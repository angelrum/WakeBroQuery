<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/9"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="system.controller.page.TicketListPage.TicketListController"
            stylesheets="@style/ClientTicketListPage.css">
   <center>
      <TableView fx:id="tableView" focusTraversable="false" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn fx:id="type" editable="false" minWidth="70.0" prefWidth="70.0" text="Тип билета" />
            <TableColumn fx:id="name" editable="false" minWidth="150.0" prefWidth="150.0" text="Название" />
            <TableColumn fx:id="equipment" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Снаряжение" />
            <TableColumn editable="false" prefWidth="75.0" text="Интервал">
               <columns>
                  <TableColumn fx:id="start" editable="false" maxWidth="70.0" minWidth="70.0" prefWidth="70.0" text="с" />
                  <TableColumn fx:id="end" editable="false" maxWidth="70.0" minWidth="70.0" prefWidth="70.0" text="по" />
               </columns>
            </TableColumn>
            <TableColumn fx:id="month" editable="false" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" text="Срок действия,мес." />
            <TableColumn fx:id="duration" editable="false" maxWidth="120.0" minWidth="120.0" prefWidth="120.0" text="Длит.сета,мин." />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <bottom>
      <Pane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="60.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="14.0" layoutY="17.0" mnemonicParsing="false" onAction="#clickOk" text="Выбрать" />
            <Button layoutX="105.0" layoutY="17.0" mnemonicParsing="false" onAction="#clickCancel" text="Отменить" />
         </children>
      </Pane>
   </bottom>
</BorderPane>
